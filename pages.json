{
	"pages": [{
		"path": "pages/cell/index",
		"style": {
			"navigationBarTitleText": "van-cell"
		}
	}, {
		"path": "pages/blank/blank",
		"style": {
			"navigationBarTitleText": "",
			"enablePullDownRefresh": false
		}
	}, {
		"path": "pages/tabbar/page1",
		"style": {
			"navigationBarTitleText": "tabbar 1"
		}
	}, {
		"path": "pages/tabbar/page2",
		"style": {
			"navigationBarTitleText": "tabbar 2"
		}
	}],
	"tabBar": {
		"list": [{
				"pagePath": "pages/tabbar/page1",
				"text": "page1"
			},
			{
				"pagePath": "pages/tabbar/page2",
				"text": "page2"
			}
		]
	},
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"easycom": {
		"autoscan": true,
		"custom": {
			"^van-(.*)": "@/uni_modules/deajax-ui/components/van-$1/index.vue"
		}
	}
}
